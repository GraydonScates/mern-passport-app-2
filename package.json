{
  "name": "mern-passport-app-2",
  "version": "1.0.0",
  "description": "MERN Passport Example App 2.0",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "install": "npm install && cd client && npm install",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client": "npm start --prefix client",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "author": "Richard G. Scates",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.7-alpha.3",
    "express-sanitizer": "^1.0.5",
    "helmet": "^3.22.0",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.14",
    "mongoose": "^5.7.5",
    "nodemailer": "^6.4.6",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.1",
    "concurrently": "^4.1.1",
    "minimist": ">=1.2.3",
    "kind-of": ">=6.0.3"
  }
}
